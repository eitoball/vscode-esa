{
    "name": "vscode-esa",
    "displayName": "vscode-esa",
    "description": "esa拡張です",
    "version": "0.0.1",
    "publisher": "kken0201",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.getAllPosts",
        "onCommand:extension.getMyPosts"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.getAllPosts",
                "title": "esa: Open From All Latest Post"
            },
            {
                "command": "extension.getMyPosts",
                "title": "esa: Open From My Post"
            }
        ]
    },
    "configuration": {
        "title": "Esa Configuration",
        "type": "object",
        "properties": {
            "esa.token": {
                "type": "string",
                "default": "",
                "description": "Copy your Personal Access Token from the website. (esa: https://[team].esa.io/user/tokens/new)"
            },
            "esa.teamName": {
                "type": "string",
                "default": "",
                "description": "Copy your Team Name."
            },
            "esa.myName": {
                "type": "string",
                "default": "",
                "description": "Copy your Screen Name."
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "request": "^2.81.0",
        "request-promise-native": "^1.0.4"
    }
}
